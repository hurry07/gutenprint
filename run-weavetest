#!/bin/sh

modes='14,7 16,8 20,8 21,7 21,8 24,4 24,6 24,8 24,12 32,8 32,16 48,4 48,6 48,8 48,12 64,4 64,8 64,16 96,2 96,3 96,6 96,8 128,8 144,6 192,2 192,3 192,6 192,8'
passes='1,1,1 2,1,1 1,2,1 1,2,2 1,4,1 2,2,1 1,4,2 2,2,2'

for mode in $modes ; do
    for pass in $passes ; do
	stuff=`echo "$mode,$pass" | awk -F, '{printf "%d %d %d %d %d\n", $1, $2, $3, $4, $5}'`
	echo -n "$stuff  "
	a=`echo $stuff | cut -f1 '-d '`
	b=`echo $stuff | cut -f2 '-d '`
	c=`echo $stuff | cut -f3 '-d '`
	d=`echo $stuff | cut -f4 '-d '`
	e=`echo $stuff | cut -f5 '-d '`
	if [ $a -ge `expr $b \* $c \* $d \* $e` ] ; then
	    eval ./escp2-weavetest $stuff 1000 | tail -1
	else
	    echo '(skipped)'
	fi
    done
done
