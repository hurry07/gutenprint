{
  # Defaults for dot-matrix printers
  ManualCopies	True
  ColorDevice	False
  Throughput	1
  VariablePaperSize	False

  Filter	"application/vnd.cups-raster 50 rastertoepson"

  MediaSize	FanfoldUS
  *MediaSize	Letter
  MediaSize	Legal
  MediaSize	A4

  {
    # EPSON 9-pin printers
    ModelNumber	0
    HWMargins	18 18 18 18

    Resolution	- 1 8 0 0 "60dpi/Fast - 60x72 DPI"
    *Resolution	- 1 8 0 0 "120dpi/Normal - 120x72 DPI"
    Resolution	- 1 8 0 0 "240dpi/Best - 240x72 DPI"

    ModelName	"EPSON 9-Pin"
    PCFileName	gpepson9.ppd
  }

  {
    # EPSON 24-pin printers
    ModelNumber	1
    HWMargins	18 18 18 18

    Resolution	- 1 8 0 0 "60dpi/Fast - 60 DPI"
    *Resolution	- 1 8 0 0 "120dpi/Fast - 120x60 DPI"
    Resolution	- 1 24 0 0 "180dpi/Normal - 180 DPI"
    Resolution	- 1 24 0 0 "360x180dpi/Normal - 360x180 DPI"
    Resolution	- 1 48 0 0 "360dpi/Best - 360 DPI"

    ModelName	"EPSON 24-Pin"
    PCFileName	gpepsn24.ppd
  }
}

{
  # Defaults for most inkjet printers
  ManualCopies	True
  ColorDevice	True
  Throughput	1
  VariablePaperSize	False

  Filter	"application/vnd.cups-raster 50 rastertoepson"
  Filter	"application/vnd.cups-command 50 commandtoepson"

  *MediaSize	Letter
  MediaSize	Legal
  MediaSize	A4
  MediaSize	A5
  MediaSize	B5
  MediaSize	EnvISOB5
  MediaSize	Env10
  MediaSize	EnvC5
  MediaSize	EnvDL
  MediaSize	EnvMonarch

  *MediaType	1 "Plain/Plain Paper"
  MediaType	1 "Special/Photo Quality InkJet Paper"
  MediaType	3 "Transparency/Transparency"
  MediaType	3 "Glossy/Photo Quality Glossy Paper"
  MediaType	3 "Film/Photo Quality Glossy Film"

  {
    # EPSON Stylus Color
    ModelNumber	2
    HWMargins	8.6 39.8 27.4 8.6

#    SimpleColorProfile	-/- 100 95 160 1.5 5 10 15

    UIConstraints	"*MediaType Transparency *Resolution 720dpi"

    Resolution	- 1 0 0 0 "360fast/Fast - 360 DPI"
    *Resolution	- 8 0 0 0 "360dpi/Normal - 360 DPI"
    Resolution	- 8 0 0 0 "720dpi/Best - 720 DPI"

    ColorModel	CMYK/Color kcmy banded 1
    ColorModel	Gray/Grayscale k chunked 1

    {
      ModelName	"EPSON Stylus Color"
      PCFileName gpstyclr.ppd
    }
  }

  {
