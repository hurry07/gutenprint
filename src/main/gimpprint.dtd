<!--
    Typical usage:

    <!DOCTYPE printdef SYSTEM "gimpprint.dtd">
    <gimp-print>
      <printdef>
        <family name="ps">
          <printer name="PostScript Level 2" driver="ps2">
            <color/>
            <model value=1/>
            <gamma value=1.000/>
            <density value=1.000/>
          </printer>
        </family>
      </printdef>
    </gimp-print>
-->



<!--                    Gimp-Print                                 -->
<!--                    Root element                               -->
<!ELEMENT  gimp-print   (printdef)                                   >
<!--         xmlns      XML default namespace
             xmlns:xsi  XML Schema Instance namespace
             xsi:schemaLocation
                        XML Schema Location                        -->
<!ATTLIST  gimp-print
             xmlns      CDATA                              #REQUIRED
             xmlns:xsi  CDATA                              #REQUIRED
             xsi:schemaLocation
                        CDATA                              #REQUIRED >


<!--                    Printdef                                   -->
<!--                    Printer definitions                        -->
<!ELEMENT  printdef     (family*)                                    >
<!--         name       Family name                                -->


<!--                    Family                                     -->
<!--                    Printer Family Driver                      -->
<!ELEMENT  family       (printer*)                                   >
<!ATTLIST  family
             name       NMTOKEN                            #REQUIRED >


<!--                    Printer                                    -->
<!--                    Printer definition                         -->
<!ELEMENT  printer          (color?, model, cyan?,
                             magenta?, yellow?, brightness?,
                             gamma?, density?, saturation?)          >
<!--         name       Model name
             driver     Family driver                              -->
<!ATTLIST  printer
             name       CDATA                              #REQUIRED
             driver     NMTOKEN                            #REQUIRED >

<!--                    Color                                      -->
<!ELEMENT  color        EMPTY                                        >
<!ATTLIST  color
             value      NMTOKEN                            #REQUIRED >

<!--                    model                                      -->
<!ELEMENT  model        EMPTY                                        >
<!--         value      Calibration value                          -->
<!ATTLIST  model
             value      NMTOKEN                            #REQUIRED >

<!--                    Cyan                                       -->
<!--                    Calibration value                          -->
<!ELEMENT  cyan         EMPTY                                        >

<!--         value      Calibration value                          -->
<!ATTLIST  cyan
             value      NMTOKEN                            #IMPLIED  >

<!--                    Magenta                                    -->
<!--                    Calibration value                          -->
<!ELEMENT  magenta      EMPTY                                        >

<!--         value      Calibration value                          -->
<!ATTLIST  magenta
             value      NMTOKEN                            #IMPLIED  >
<!--                    Yellow                                     -->

<!--                    Calibration value                          -->
<!ELEMENT  yellow       EMPTY                                        >

<!--         value      Calibration value                          -->
<!ATTLIST  yellow
             value      NMTOKEN                            #IMPLIED  >

<!--                    brightness                                 -->
<!ELEMENT  brightness   EMPTY                                        >
<!--         value      Calibration value                          -->
<!ATTLIST  brightness
             value      NMTOKEN                             #IMPLIED >

<!--                    gamma                                      -->
<!ELEMENT  gamma        EMPTY                                        >
<!--         value      Calibration value                          -->
<!ATTLIST  gamma
             value      NMTOKEN                            #REQUIRED >

<!--                    density                                    -->
<!ELEMENT  density      EMPTY                                        >
<!--         value      Calibration value                          -->
<!ATTLIST  density
             value      NMTOKEN                            #REQUIRED >

<!--                    saturation                                 -->
<!ELEMENT  saturation   EMPTY                                        >
<!--         value      Calibration value                          -->
<!ATTLIST  saturation
             value      NMTOKEN                            #REQUIRED >



<!-- ============================================================= -->
<!--                    End of GIMP-Print Printers DTD             -->
<!-- ============================================================= -->
